#pragma once


class Mur {
public:
	int* getX();
	int* getY();
private:

	//tablica int przechowywujaca dane na jakich wspolrzednych znajduje sie obiekt mur - blokuje gracza;
	int y_coordinate[5] = {
		2,3,4,5,6
	};
	int x_coordinate[5]={
		55, 56, 57, 58, 59
	};
};



// PLIK MUR CPP


#include "stdafx.h"
#include "Mur.h"

int * Mur::getX()
{
	int*wsk = x_coordinate;
	return wsk;
}

int * Mur::getY()
{
	int*wsk = y_coordinate;
	return wsk;
}
